<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <object class="GtkShortcutsWindow" id="shortcuts_overview">
    <property name="modal">True</property>
    <child>
      <object class="GtkShortcutsSection">
        <property name="visible">True</property>
        <property name="section-name">shortcuts</property>
        <property name="max-height">10</property>
        <child>
          <object class="GtkShortcutsGroup">
            <property name="visible">True</property>
            <property name="title" translatable="yes" context="Shortcut window description">Anwendung</property>
            <child>
              <object class="GtkShortcutsShortcut" id="entry1">
                <property name="visible">True</property>
                <property name="title" translatable="yes" context="Shortcut window description">Beenden</property>
                <property name="accelerator">&lt;Primary&gt;Q</property>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="GtkShortcutsGroup">
            <property name="visible">True</property>
            <property name="title" translatable="yes" context="Shortcut window description">Speichern</property>
            <child>
              <object class="GtkShortcutsShortcut" id="entry2">
                <property name="visible">True</property>
                <property name="title" translatable="yes" context="Shortcut window description">Bild speichern</property>
                <property name="accelerator">&lt;Primary&gt;S</property>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
